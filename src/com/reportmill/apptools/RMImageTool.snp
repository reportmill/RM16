<?xml version="1.0" encoding="UTF-8"?>
<SpringView width="275" height="325" PrefWidth="275" PrefHeight="325">
  <Label x="10" y="10" width="30" height="20" text="Key:" />
  <TextField Name="KeyText" x="40" y="10" width="172" height="20" />
  <Button Name="KeysButton" x="215" y="10" width="55" height="20" text="Keys">
    <font name="Arial" size="10" />
  </Button>
  <Label x="10" y="35" width="50" height="20" text="Padding:" />
  <TextField Name="PaddingText" x="60" y="35" width="72" height="20" Align="CENTER" />
  <CheckBox Name="GrowToFitCheckBox" x="10" y="65" width="200" height="20" text="Grow to Fit" />
  <CheckBox Name="PreserveRatioCheckBox" x="10" y="85" width="200" height="20" text="Preserve Image Aspect Ratio" />
  <TitleView x="6" y="115" width="260" height="48" Title="Rounding Radius">
    <SpringView width="260" height="48" PrefWidth="260" PrefHeight="48">
      <ThumbWheel Name="RoundingThumb" x="14" y="7" width="180" height="16" absmin="0" round="1" />
      <TextField Name="RoundingText" x="199" y="5" width="50" height="20" Align="CENTER" />
    </SpringView>
  </TitleView>
  <TextArea Name="TypeLabel" x="10" y="185" width="255" height="40" />
  <Button Name="SaveButton" x="10" y="300" width="100" height="20" text="Save...">
    <font name="Arial" size="10" />
  </Button>
  <Button Name="JPEGButton" x="165" y="300" width="100" height="20" text="Convert to Jpeg">
    <font name="Arial" size="10" />
  </Button>
</SpringView>